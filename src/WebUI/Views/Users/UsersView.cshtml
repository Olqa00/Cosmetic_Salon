@model IReadOnlyList<CosmeticSalon.Application.Users.ViewModels.User>

<body>

<h1>
    Users View
</h1>

@foreach (var user in Model)
{
    <div>
        @user.Id
    </div>
    <div>
        @user.Username
    </div>
    <div>
        @user.Fullname
    </div>
    <div>
        @user.Role
    </div>
})
</body>